{
  "name": "citizen-portal",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "configure": "ts-node setenv.ts",
    "debug": "yarn start --configuration debug --port 8080",
    "start": "yarn run configure --environment=dev && ng serve --proxy-config proxy.conf.json",
    "build-dev": "yarn run configure --environment=dev && ng build --source-map=false",
    "build-prod": "yarn run configure --environment=prod && ng build --configuration production --source-map=false",
    "api:clean": "npx del-cli \"src/app/api/**\" \"!src/app/api/templates\"",
    "api:download": "npx download -o src/app/api http://localhost:5080/swagger/v1/swagger.json",
    "api:generate": "npx openapi-generator-cli generate -i src/app/api/swagger.json -g typescript-angular -t src/app/api/templates --additional-properties=modelFileSuffix=.model -o src/app/api",
    "api:regenerate": "yarn run api:clean && yarn run api:download && yarn run api:generate",
    "api:r": "yarn run api:regenerate",
    "oidc-config-dev:download": "npx download -o src/assets --filename oidc.config.json https://idtest.gov.bc.ca/login/.well-known/openid-configuration",
    "oidc-config-prod:download": "npx download -o src/assets --filename oidc.config.json https://id.gov.bc.ca/login/.well-known/openid-configuration",
    "test": "ng test",
    "lint": "ng lint",
    "audit": "yarn-audit-fix --dry-run",
    "audit:fix": "yarn-audit-fix",
    "e2e": "ng e2e",
    "coverage": "ng test --browsers=ChromeHeadless --watch=false --code-coverage",
    "cypress": "./node_modules/.bin/cypress run --project e2e",
    "cypress-open": "./node_modules/.bin/cypress open --project e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.12",
    "@angular/cdk": "^16.2.11",
    "@angular/common": "^16.2.12",
    "@angular/compiler": "^16.2.12",
    "@angular/core": "^16.2.12",
    "@angular/flex-layout": "^15.0.0-beta.42",
    "@angular/forms": "^16.2.12",
    "@angular/material": "^16.2.11",
    "@angular/platform-browser": "^16.2.12",
    "@angular/platform-browser-dynamic": "^16.2.12",
    "@angular/router": "^16.2.12",
    "@azure/ai-form-recognizer": "^3.1.0",
    "@babel/runtime": "^7.20.6",
    "@bcgov/bc-sans": "^1.0.1",
    "@ngrx/effects": "^16.3.0",
    "@ngrx/store": "^16.3.0",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "angular-auth-oidc-client": "^16.0.1",
    "bootstrap": "^5.3.2",
    "colors": "^1.4.0",
    "dotenv": "^10.0.0",
    "lodash": "^4.17.21",
    "luxon": "^3.4.3",
    "ng-busy": "^16.1.1",
    "ng2-pdf-viewer": "^10.0.0",
    "ngx-mask": "^16.3.9",
    "ngx-material-timepicker": "^13.1.1",
    "ngx-progressbar": "^11.1.0",
    "rxjs": "~7.5.5",
    "tslib": "^2.3.0",
    "yargs": "^17.1.0",
    "zone.js": "~0.13.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.9",
    "@angular-eslint/builder": "16.2.0",
    "@angular-eslint/eslint-plugin": "16.2.0",
    "@angular-eslint/eslint-plugin-template": "16.2.0",
    "@angular-eslint/schematics": "16.2.0",
    "@angular-eslint/template-parser": "16.2.0",
    "@angular/cli": "^16.2.9",
    "@angular/compiler-cli": "^16.2.12",
    "@ngrx/eslint-plugin": "^16.3.0",
    "@openapitools/openapi-generator-cli": "^2.5.2",
    "@types/faker": "^5.5.7",
    "@types/jasmine": "~3.8.2",
    "@types/jasminewd2": "~2.0.10",
    "@types/node": "^16.4.13",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "cypress": "^8.2.0",
    "del-cli": "^5.0.0",
    "download-cli": "^1.1.1",
    "eslint": "^8.39.0",
    "faker": "^5.5.3",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "^6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "protractor": "~7.0.0",
    "ts-node": "^10.9.1",
    "typescript": "~4.9.5",
    "yarn": "^1.22.11",
    "yarn-audit-fix": "^9.3.7"
  },
  "resolutions": {
    "postcss": "^8.1.0"
  }
}
