{
  "name": "citizen-portal",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "configure": "ts-node setenv.ts",
    "start": "npm run configure -- --environment=dev && ng serve --proxy-config proxy.conf.json",
    "build-dev": "npm run configure -- --environment=dev && ng build --source-map=false",
    "build-prod": "npm run configure -- --environment=prod && ng build --prod  --source-map=false",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "coverage": "ng test --browsers=ChromeHeadless --watch=false --code-coverage",
    "cypress": "./node_modules/.bin/cypress run --project e2e",
    "cypress-open": "./node_modules/.bin/cypress open --project e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/cdk": "^12.2.0",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/forms": "~12.2.0",
    "@angular/material": "^12.2.0",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/router": "~12.2.0",
    "@azure/ai-form-recognizer": "^3.1.0",
    "@bcgov/bc-sans": "^1.0.1",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "bootstrap": "^5.1.0",
    "colors": "^1.4.0",
    "dotenv": "^10.0.0",
    "keycloak-angular": "^8.3.0",
    "keycloak-js": "^15.0.0",
    "ng-busy": "^12.0.1",
    "ngx-mask": "^12.0.0",
    "ngx-progressbar": "^6.1.1",
    "rxjs": "~7.3.0",
    "tslib": "^2.3.0",
    "yargs": "^17.1.0",
    "yarn": "^1.22.11",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.0",
    "@angular/cli": "~12.2.0",
    "@angular/compiler-cli": "~12.2.0",
    "@types/faker": "^5.5.7",
    "@types/jasmine": "~3.8.2",
    "@types/jasminewd2": "~2.0.10",
    "@types/node": "^16.4.13",
    "codelyzer": "^6.0.2",
    "cypress": "^8.2.0",
    "faker": "^5.5.3",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "^6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "npx": "^10.2.2",
    "protractor": "~7.0.0",
    "ts-node": "~10.1.0",
    "tslint": "~6.1.0",
    "typescript": "~4.3.5"
  }
}
